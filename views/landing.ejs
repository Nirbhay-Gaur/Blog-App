<%- include("./partials/landingHeader") %>

<div class="ui container main center aligned">
    <div class="ui huge header" id="headL" >
        <div id = "hello">Hello!</div>
        <div id="wlcm">Welcome</div>
    </div>
    <span class="animation-btn">
        <a class="ui huge violet button animated fade" href="/blogs">
            <span class="visible content">Enter</span>
            <span class="hidden content"><i class="sign-in icon"></i></span>
        </a>
    </span>
</div>

<script>
    function fade(){
    $("#hello").fadeIn(1000);
    setTimeout(function(){
        $("#hello").fadeOut(500);
        setTimeout(function(){
            $("#wlcm").fadeIn(1000);
        }, 500);
    },1500);
    setTimeout(function(){
        $(".animation-btn").fadeIn(1000);
    }, 3000);
    return false;
}
fade();
</script>

